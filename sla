local teleportPosition = Vector3.new(0, 10, 0)

-- Função que verifica se um objeto possui o nome "Flags"
local function isFlag(object)
    return object.Name == "Flags"
end

-- Função que teleporta um objeto para a posição definida
local function teleportObject(object)
    object.Position = teleportPosition
end

-- Percorre todos os objetos no workspace
for _, object in ipairs(workspace:GetChildren()) do
    if isFlag(object) then
        teleportObject(object)
    end
end
